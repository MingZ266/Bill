<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- 所使用的圆角的半径. -->
    <dimen name="cornerRadius">12dp</dimen>
    <!-- 图标的边长. -->
    <dimen name="iconSideLen">30dp</dimen>
    <!-- 小号文本，用于小标题的文本尺寸. -->
    <dimen name="smallText">14sp</dimen>
    <!-- 中号文本，用于显示多数文本的大小. -->
    <dimen name="mediumText">18sp</dimen>
